@model BKStudent.Models.Student


@{
    ViewData["Title"] = "search_by_proberty";
}

<h2 class="text-success" style="font-family : 'Times New Roman'">Tìm kiếm sinh viên</h2>

<hr />
<div class="container" style="text-align: center">
    <form class="form-horizontal" id="form1" runat="server">
        <div class="col-md-12">
            <div class="form-group">
                <label class="control-label col-sm-2" >Tim theo ten :</label>
                <div class="col-sm-10">
                    <input type="text" class="form-control" runat="server" id="name" >
                </div>
            </div>
        </div>
    </form>
</div>


<div>
    <form asp-action="Index">
        <input type="submit" class="btn btn-default" value="Quay lại danh sách" asp-action="Index" />
    </form>
</div>



@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
